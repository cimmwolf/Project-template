{
  "sources": [],
  "extraDependencies": [
    "robots.txt",
    "node_modules/@webcomponents/webcomponentsjs/*.js",
    "node_modules/@webcomponents/webcomponentsjs/bundles/*.js",
    "!node_modules/@webcomponents/webcomponentsjs/gulpfile.js"
  ],
  "lint": {
    "rules": [
      "polymer-3"
    ]
  },
  "builds": [
    {
      "name": "es5",
      "bundle": false,
      "js": {
        "compile": true,
        "minify": true,
        "transformModulesToAmd": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      }
    },
    {
      "name": "es6",
      "bundle": false,
      "js": {
        "compile": false,
        "minify": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      }
    }
  ]
}
